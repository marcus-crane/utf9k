{
  "title": "{{ .Site.Title }}",
  "hugo": {
    "version": "{{ hugo.Version }}",
    "environment": "{{ hugo.Environment }}",
    "buildDate": "{{ hugo.BuildDate }}"
  },
  {{ if not .Site.IsServer }}"git": {
    "commit": "{{ .GitInfo.AbbreviatedHash }}",
    "commitURL": "https://github.com/marcus-crane/utf9k/commit/{{ .GitInfo.Hash }}",
    "subject": "{{ .GitInfo.Subject }}"{{ if getenv "NETLIFY"}},
    "branch": "{{ getenv "BRANCH" }}"{{ end }}
  },
  {{ end }}"site": {
    "buildingDrafts": "{{ .Site.BuildDrafts }}",
    "baseURL": "{{ .Site.BaseURL }}",
    "localServer": "{{ .Site.IsServer }}",
    "lastChange": "{{ .Site.LastChange }}",
    "pageCount": "{{ len .Site.AllPages }}"
  }
}
